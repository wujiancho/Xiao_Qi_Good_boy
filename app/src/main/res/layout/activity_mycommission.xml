                        android:layout_width="@dimen/dp_88"
                        android:layout_height="@dimen/dp_88"
                        android:layout_marginRight="@dimen/dp_15"
                        android:src="@drawable/logo"
                        android:layout_gravity="right"
                        android:layout_marginBottom="@dimen/dp_10"
